public interface TagDAO {
	public boolean addTag(Tag tag) throws Exception; 
	public boolean delTag(String sql) throws Exception; 
	public boolean updateTag(String sql) throws Exception;
	public ArrayList<Tag> getTag(String sql) throws Exception;

}
